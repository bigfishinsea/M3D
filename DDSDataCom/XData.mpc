project(*idl): dcps {
  TypeSupport_Files {
    XData.idl
  }
 
  custom_only = 1
}
 
project(*publisher) : dcpsexe_with_tcp {
  exename   = publisher
  after    += *idl
 
  TypeSupport_Files {
    XData.idl
  }
 
  Source_Files {
    XDataPublisher.cpp
  }
}
 
project(*subscriber) : dcpsexe_with_tcp {
  exename   = subscriber
  after    += *publisher
 
  TypeSupport_Files {
    XData.idl
  }
 
  Source_Files {
    XDataSubscriber.cpp
    XDataReaderListener.cpp
  }
}
